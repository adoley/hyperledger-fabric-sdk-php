{
  "name": "americanexpress/fabric-sdk-php",
  "description": "Hyperledger fabric client SDK in PHP",
  "type": "library",
  "homepage": "http://americanexpress.io/",
  "keywords": ["hyperledger", "fabric", "sdk", "php", "grpc", "protobuf"],
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "AmericanExpress\\FabricClient\\": "fabric-client/src/lib",
      "Common\\": "protobuf/dist/Common",
      "Google\\": "protobuf/dist/Google",
      "GPBMetadata\\": "protobuf/dist/GPBMetadata",
      "Msp\\": "protobuf/dist/Msp",
      "Orderer\\": "protobuf/dist/Orderer",
      "Protos\\": "protobuf/dist/Protos"
    }
  },
  "autoload-dev": {
    "classmap": [
      "test"
    ],
    "files": [
      "fabric-client/Index.php"
    ]
  },
  "require": {
    "php": "^7.1",
    "ext-grpc": "*",
    "grpc/grpc": "^v1.3.0",
    "google/protobuf": "^v3.3.0",
    "mdanter/ecc": "^v0.4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "scripts": {
    "coverage": "vendor/bin/phpunit --coverage-text",
    "test": "vendor/bin/phpunit"
  }
}