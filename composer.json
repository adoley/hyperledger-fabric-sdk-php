{
  "name": "fabric-sdk-php",
  "description": "Hyperledger fabric client SDK in PHP",
  "type": "library",
  "homepage": "http://americanexpress.io/",
  "keywords": ["hyperledger", "fabric", "sdk", "php", "grpc", "protobuf"],
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "AmericanExpress\\FabricClient\\msp\\": "fabric-client/src/lib/msp",
      "AmericanExpress\\FabricClient\\": "fabric-client/src/lib",
      "Common\\": "fabric-client/src/lib/protosPHP/Common",
      "Google\\": "fabric-client/src/lib/protosPHP/Google",
      "GPBMetadata\\": "fabric-client/src/lib/protosPHP/GPBMetadata",
      "Msp\\": "fabric-client/src/lib/protosPHP/Msp",
      "Orderer\\": "fabric-client/src/lib/protosPHP/Orderer",
      "Protos\\": "fabric-client/src/lib/protosPHP/Protos"
    }
  },
  "require": {
    "grpc/grpc": "^v1.3.0",
    "google/protobuf": "^v3.3.0",
    "mdanter/ecc": "^v0.4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "6.0.*"
  }
}